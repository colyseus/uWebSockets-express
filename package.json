    "@types/path-to-regexp": "^1.7.0",
    "@types/serve-index": "^1.9.0",
    "axios": "^1.4.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "esbuild": "^0.23.0",
    "express-session": "^1.17.2",
    "fast-glob": "^3.3.2",
    "mocha": "^9.0.1",
    "pug": "^3.0.2",
    "serve-index": "^1.9.1",
    "serve-static": "^1.14.1",
